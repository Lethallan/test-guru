<div class="form-group">
  <%= form_with model: [:admin, @test], local: true do |form| %>
    <%= render 'shared/errors', resource: @test %>
    <%= form.hidden_field :user_id, :value => current_user.id %>

    <p>
      <%= form.label :title %><br>
      <%= form.text_field :title, id: :test_title, class: 'form-control' %>
    </p>

    <p>
      <%= form.label :level %><br>
      <%= form.number_field :level, id: :test_level, class: 'form-control' %>
    </p>

    <p>
      <%= form.label :category_id %><br>
      <%= form.collection_select :category_id, Category.all, :id, :topic, prompt: true, class: 'form_control' %>
    </p>

    <p><%= form.submit t('.submit'), class: 'btn btn-primary' %></p>
  <% end %>
</div>